<div class="container">

  <div class="jumbotron">
    <h1 class="text-center"><span class="fas fa-book-reader"></span> Tutor Finder</h1>
    <hr>
    <h2 class="text-center">Find a perfect Tutor for your need!</h2>
    <br>

    <div class="text-center">


    </div>
    <div class="main">

      <div class="input-group">
        <input type="text" class="form-control" placeholder="Search for Tutor">
        <div class="input-group-append">
          <button class="btn btn-secondary" type="button">
            <i class="fa fa-search"></i>
          </button>
        </div>
      </div>

    </div>
  </div>

  <div class="container">
    <div class="card-deck">
      {{#each tutor}}
        {{> tutor-block}}
      {{/each}}
    </div>
  </div>
</div>





  






